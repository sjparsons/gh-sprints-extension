{
  "name": "GH sprints",
  "version": "0.0.1",
  "description": "Make it easy to manage sprints with GitHub",
  "icons": {
    "24": "icons/checklist.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://github.braintreeps.com/*/*/projects/*",
        "https://github.com/*/*/projects/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "constants.js",
        "sprint.js"
      ]
    },
    {
        "matches": [
          "https://github.braintreeps.com/*/*/issues",
          "https://github.braintreeps.com/*/*/issues?*",
          "https://github.com/*/*/issues",
          "https://github.com/*/*/issues?*"
        ],
        "css": [
          "styles.css"
        ],
        "js": [
          "jquery-3.5.1.min.js",
          "constants.js",
          "issues.js"
        ]
      }
  ]
}
